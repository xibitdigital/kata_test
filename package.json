{
  "name": "gilded-rose-kata",
  "version": "1.0.0",
  "description": "Gilded Rose kata in TypeScript",
  "scripts": {
    "precompile": "rimraf app/**/*.js test/**/*.js",
    "compile": "tsc",
    "test": "jest --coverage",
    "test-watch": "jest --watchAll",
    "precommit": "pretty-quick --staged"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@types/babel-core": "^6.25.6",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@types/ramda": "^0.26.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-polyfill": "^6.26.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.6.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/test/.*(spec.ts))$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "engines": {
    "node": ">=10.6.0"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  }
}
